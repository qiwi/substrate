{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src/main/ts/",
    "baseUrl": "./src/main/ts/",
    "target": "es6",
    "outDir": "target/cjs",
    "tsBuildInfoFile": "./buildcache/.tsbuildinfo",
    "lib": [
      "es6",
      "es2015.promise",
      "dom"
    ]
  },
  "include": [
    "src/main/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
