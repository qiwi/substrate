{
  "name": "@qiwi/substrate-types",
  "version": "1.0.0",
  "main": "lib/es5/index.js",
  "types": "typings/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib flow-typed",
    "build": "yarn build_es5 && yarn build_es6 && yarn build_ts",
    "build_es5": "mkdir -p lib/es5 && tsc -p tsconfig.json --target ES5 --outDir lib/es5",
    "build_es6": "mkdir -p lib/es6 && tsc -p tsconfig.json --target ES6 --outDir lib/es6",
    "build_ts": "cp -r src/main/ lib/ts/",
    "flowfix": "replace-in-file --configFile=./flowfix.config.js",
    "test": "jest --config=jest.config.json",
    "generate": "ts-node -e 'require(\"./src/gen\",).run()'",
    "dtsgen": "dts-generator --project ./ --out typings/index.d.ts --prefix @qiwi/substrate-types/lib/es5 --name @qiwi/substrate-types --main @qiwi/substrate-types/lib/es5/index",
    "flowgen": "flowgen typings/index.d.ts --output-file typings/index.flow.js"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "typings/",
    "lib/"
  ],
  "dependencies": {},
  "devDependencies": {
    "dts-generator": "^3.0.0",
    "@types/jest": "^23.3.12",
    "ts-loader": "^5.3.3",
    "ts-to-flow": "^0.0.3",
    "typescript": "3.2.2",
    "ts-node": "^7.0.1",
    "lodash": "^4.17.11",
    "replace-in-file": "^3.4.3",
    "flowgen": "^1.3.0"
  }
}
