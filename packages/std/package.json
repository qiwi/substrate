    "build:esm": "yarn build:tsc:esm",
    "build:ts": "yarn build:copy:src",
    "build:docs": "yarn build:typedoc",
    "build:stamp": "yarn build:buildstamp",
    "test": "yarn test:unit",
    "test:unit": "jest --config=jest.config.json --detectOpenHandles --forceExit"
