{
  "branch": "master",
  "verifyConditions": [
    "@semantic-release/changelog",
    "@semantic-release/npm",
    "@semantic-release/git",
    "@qiwi/semantic-release-gh-pages-plugin"
  ],
  "prepare": [
    "@semantic-release/changelog",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "publish": [
    "@semantic-release/npm",
    "@semantic-release/github",
    [
      "@qiwi/semantic-release-gh-pages-plugin",
      {
        "branch": "gh-pages",
        "msg": "updated {{=it.nextRelease.gitTag}}",
        "src": "./packages/abstract/docs",
        "dst": "./abstract"
      }
    ]
  ]
}
