{
  "name": "@qiwi/substrate-abstract",
  "description": "Abstract classes",
  "version": "1.18.0",
  "main": "target/es5/index.js",
  "source": "target/ts/index.ts",
  "types": "target/es5/index.d.ts",
  "typescript": {
    "definition": "target/es5/index.d.ts"
  },
  "typesVersions": {
    ">=3.1": {
      "*": [
        "target/es5/index.d.ts",
        "src/test/ts/*"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf target",
    "build": "yarn aliasgen && yarn build:es5 && yarn build:es6 && yarn build:ts && yarn build:libdef && yarn docs",
    "build:es5": "mkdir -p target/es5 && tsc -p tsconfig.json --target ES5 --outDir target/es5",
    "build:es6": "mkdir -p target/es6 && tsc -p tsconfig.json --target ES6 --outDir target/es6",
    "build:ts": "cp -r src/main/ts/ target/ts/",
    "build:libdef": "dts-generator --project ./ --out typings/index.d.ts --prefix @qiwi/substrate-abstract/target/es5 --name @qiwi/substrate-abstract --main @qiwi/substrate-abstract/target/es5/index && libdeffix --dts=./typings/index.d.ts --prefix=@qiwi/substrate-abstract/target/es5 && flowgen typings/index.d.ts --output-file flow-typed/index.flow.js",
    "test": "yarn test:libdefs && yarn jest",
    "test:libdefs": "cp typings/index.d.ts src/test/ts/libdef/index.d.ts && yarn dtslint",
    "jest": "jest --config=jest.config.json",
    "dtslint": "dtslint src/test/ts/libdef --localTs ../../node_modules/typescript/lib --expectOnly",
    "aliasgen": "node -r esm ../../scripts/js/alias-gen.js",
    "docs": "typedoc --readme README.md --tsconfig ./tsconfig.json ./src/main --ignoreCompilerErrors || exit 0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "typings",
    "flow-typed",
    "target"
  ],
  "devDependencies": {
    "@qiwi/uniconfig": "3.5.1",
    "@types/bluebird": "3.5.32",
    "@types/config": "0.0.36",
    "@types/jest": "26.0.13",
    "@types/lodash": "4.14.161",
    "@types/node": "14.10.1",
    "@types/parsimmon": "1.10.2",
    "@types/underscore": "1.10.24",
    "bluebird": "3.7.2",
    "conf": "7.1.2",
    "config": "3.3.1",
    "lodash": "4.17.20",
    "underscore": "1.11.0"
  },
  "dependencies": {
    "@qiwi/substrate-types": "1.46.0",
    "@qiwi/substrate-infra": "1.0.0"
  },
  "peerDependencies": {},
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/substrate.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/qiwi/substrate/issues"
  },
  "optionalDependencies": {}
}
