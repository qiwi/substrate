{
  "name": "@qiwi/substrate",
  "version": "1.20.11",
  "main": "target/es5/index.js",
  "source": "target/ts/index.ts",
  "types": "target/es5/index.d.ts",
  "typescript": {
    "definition": "target/es5/index.d.ts"
  },
  "typesVersions": {
    ">=4": {
      "*": [
        "target/es5/index.d.ts",
        "src/test/ts/*"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf target buildcache flow-typed typings docs coverage buildstamp.json",
    "build": "concurrently -n yarn:build:es5 yarn:build:es6 yarn:build:ts yarn:build:libdef yarn:build:stamp",
    "build:es5": "mkdirp target/es5 && tsc -p ./tsconfig.json --target ES5 --outDir target/es5",
    "build:es6": "mkdirp target/es6 && tsc -p ./tsconfig.json --target ES6 --outDir target/es6",
    "build:ts": "cpy src/main/ts/ target/ts/",
    "build:libdef": "../../node_modules/.bin/libdefkit --tsconfig=tsconfig.json",
    "testgen": "cp typings/index.d.ts src/test/ts/libdef/index.d.ts && rsync -a ../types/src/test/ts/libdef/I*.ts ./src/test/ts/libdef/ && rsync -a ../abstract/src/test/ts/unit/*.ts ./src/test/ts/unit/ && rsync -a ../std/src/test/ts/unit/*.ts ./src/test/ts/unit/",
    "test": "yarn testgen && yarn dtslint && yarn jest",
    "jest": "jest --config=jest.config.json --detectOpenHandles --forceExit --passWithNoTests",
    "dtslint": "dtslint src/test/ts/libdef --localTs ../../node_modules/typescript/lib --expectOnly",
    "build:stamp": "buildstamp --git --out.path buildstamp.json"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "typings",
    "flow-typed",
    "target",
    "buildstamp.json"
  ],
  "dependencies": {
    "@qiwi/substrate-abstract": "1.20.6",
    "@qiwi/substrate-std": "1.3.6",
    "@qiwi/substrate-types": "1.53.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@qiwi/substrate-infra": "1.2.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.15",
    "fs-extra": "^10.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/substrate.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/qiwi/substrate/issues"
  }
}
